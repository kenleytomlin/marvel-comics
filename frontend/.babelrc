{
  "presets": [
    "latest",
    "react"
  ],
  "plugins": [
    "transform-decorators-legacy",
    "transform-class-properties",
    "transform-runtime",
    "transform-es2015-classes",
    "transform-object-rest-spread",
    "react-hot-loader/babel",
    ["babel-plugin-styled-components", {
      "ssr": true
    }]
  ]
}
